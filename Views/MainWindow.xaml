<Window x:Class="Garbage_Organizer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Garbage_Organizer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,0,0,-6">
        <Menu HorizontalAlignment="Center" Height="30" VerticalAlignment="Top" Width="800">
            <MenuItem Header="Files" Height="28" Width="86" Click="MenuItem_Click">
                <ContentControl Content="ContentControl"/>
            </MenuItem>
        </Menu>
        <Label Content="Select files..." HorizontalAlignment="Left" Height="35" Margin="139,129,0,0" VerticalAlignment="Top" Width="441"/>
        <Button Content="Open" HorizontalAlignment="Left" Height="35" Margin="635,129,0,0" VerticalAlignment="Top" Width="128" Click="Button_Click_1"/>
        <ProgressBar Height="10" Width="400" Margin="200,215,200,215"/>

    </Grid>
</Window>
